---
import Layout from "../layouts/Layout.astro";
import {getEntry} from 'astro:content'
import PhotoSwipeAll from "../components/PhotoSwipeAll.astro";

const page = await getEntry('pages', 'galerie')
---
<Layout title="Gallerie">
  <main class="page-width mx-auto px-4 md:px-0">
    <PhotoSwipeAll images={page.data.gallery} />
  </main>
</Layout>
